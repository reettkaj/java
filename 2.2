//java
function collectNames() {
    const count = parseInt(prompt("Number of participants:"));
    const names = [];
    for (let i = 0; i < count; i++) {
        names.push(prompt(`Enter name for participant ${i + 1}:`));
    }
    names.sort();
    console.log("Participants in alphabetical order:");
    names.forEach(name => console.log(name));
}

collectNames();
