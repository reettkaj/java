//java
'use strict';

// Fetching a random joke from the chucknorris.io API
fetch('https://api.chucknorris.io/jokes/random')
  .then(response => {
    if (!response.ok) {
      throw new Error('Network response was not ok');
    }
    return response.json();
  })
  .then(data => {
    // Print the joke to the console
    console.log(data.value); // 'value' contains the joke
  })
  .catch(error => {
    console.error('There was an error fetching the joke:', error);
  });
